    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <IsPackable>True</IsPackable>
    <PackageId>King.Of.Tokyo.WebApi.Team1</PackageId>
    <Authors>Adriel Amorim, Jorge Chavez</Authors>
    <Company>Jalasoft</Company>
    <Description>KOT WebApi</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>DevOps, AT, Bootcamp, Jalasoft</PackageTags>
